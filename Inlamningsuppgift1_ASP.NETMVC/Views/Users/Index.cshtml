@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Home Page";
}
<div class="container">
    <h1>Users</h1>

    <p>
        <a asp-area="Identity" asp-page="/Account/Register">Create New</a>
    </p>
    <div class="d-flex row">

        @foreach (var user in Model)
        {

            <div class="col-3 mt-3">
                <div class="card">
                    <div class="card-body">
                        <img src="" alt="" class="img-fluid rounded-circle w-50 mb-3">
                        <h4>@user.DisplayName</h4>
                        <h6>@user.Email</h6>
                        <p>@user.DateOfBirth</p>
                        <p>@user.Role</p>
                        <div>
                            <a asp-action="Edit" asp-route-id="@user.Id">Edit</a> |
                            <a asp-action="Delete" asp-route-id="@user.Id">Delete</a>
                        </div>
                    </div>
                </div>
            </div>



        }

    </div>
</div>

